return {
    platform = Commodore_64_Platform,
    install_image = 'blank.d64',
    setup_image = 'setup.d64',
    system_image = 'device8',
    dist_image = 'dist.d64',
    distrepos_hg = [[
      https://bitbucket.org/catseye/bubble-escape
      https://bitbucket.org/catseye/c64-demo-depot
      https://bitbucket.org/catseye/disksumo
      https://bitbucket.org/catseye/sixtypical
    ]],
    setup_script = [[
cat >vicerc <<EOF
[C64]
VICIIDoubleScan=1
VICIIDoubleSize=1
EOF
cp distrepos/bubble-escape/bin/* device8/
cp distrepos/c64-demo-depot/petulant/petulant.prg device8/petulant
cp distrepos/c64-demo-depot/ribos/ribos.prg device8/ribos
cp distrepos/disksumo/bin/disksumo.prg device8/disksumo
    ]],
    dist_instructions = [[
Use Commodore DOS copy commands to copy files from drive 8 to drive 9.
    ]],
}
