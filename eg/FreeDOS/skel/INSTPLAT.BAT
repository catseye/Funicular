CALL D:\INSTDJGP.BAT

CD C:\
IF EXIST C:\TOOLS\NUL GOTO AlreadyTools
MKDIR TOOLS
CD C:\TOOLS
UNZIP D:\TED3.ZIP
UPX -9 D:\YASM120.EXE -oC:\TOOLS\YASM.EXE
COPY D:\YASMLCNS.TXT C:\TOOLS\
COPY D:\INITFLOP.BAT C:\TOOLS\
COPY D:\PUTFLOPY.BAT C:\TOOLS\
COPY D:\POPUFLOP.BAT C:\TOOLS\

MKDIR C:\TOOLS\DOS2UNIX
CD C:\TOOLS\DOS2UNIX
UNZIP D:\d2u740pm.zip
COPY C:\TOOLS\DOS2UNIX\BIN\*.EXE C:\TOOLS\

C:\TOOLS\UNIX2DOS C:\TOOLS\YASMLCNS.TXT

:AlreadyTools
CD C:\
MKDIR PROJECTS
CD PROJECTS

IF EXIST C:\PROJECTS\SHELTA GOTO AlreadyShelta

ECHO ------ SHELTA ------

UNZIP D:\SHELTA12.ZIP
RENAME SHELTA-1.042 SHELTA
CD SHELTA
C:\TOOLS\UNIX2DOS README.MD
C:\TOOLS\UNIX2DOS MAKEFILE
C:\TOOLS\UNIX2DOS DOC\*.*
C:\TOOLS\UNIX2DOS EG\*.*
C:\TOOLS\UNIX2DOS LIB\8086\*.*
C:\TOOLS\UNIX2DOS LIB\GUPI\*.*
C:\TOOLS\UNIX2DOS SRC\*.*

:AlreadyShelta
IF EXIST C:\PROJECTS\ILLGOL GOTO AlreadyILLGOL

ECHO ------ ILLGOL ------

CD C:\PROJECTS
UNZIP D:\ILLGOLGM.ZIP
RENAME ILLGOL-G.GET ILLGOLGM
CD ILLGOLGM
RENAME 1.?IL ILLGOL
RENAME 2.?IL ILLGOLA2
RENAME 3.?IL ILLBERON
RENAME 4.?OP ILLGOLPP
C:\TOOLS\UNIX2DOS README.MAR 
CD C:\PROJECTS\ILLGOLGM\ILLGOL
C:\TOOLS\UNIX2DOS DOC\*.*
C:\TOOLS\UNIX2DOS SRC\*.*
C:\TOOLS\UNIX2DOS PRJ\*.*
CD C:\PROJECTS\ILLGOLGM\ILLGOLGA2
C:\TOOLS\UNIX2DOS DOC\*.*
C:\TOOLS\UNIX2DOS SRC\*.*
C:\TOOLS\UNIX2DOS PRJ\*.*
CD C:\PROJECTS\ILLGOLGM\ILLBERON
C:\TOOLS\UNIX2DOS DOC\*.*
C:\TOOLS\UNIX2DOS SCRIPT\*.*
CD C:\PROJECTS\ILLGOLGM\ILLGOLPP
C:\TOOLS\UNIX2DOS DOC\*.*
C:\TOOLS\UNIX2DOS SCRIPT\*.*
C:\TOOLS\UNIX2DOS SRC\*.*
C:\TOOLS\UNIX2DOS PRJ\*.*
C:\TOOLS\UNIX2DOS COPING

:AlreadyILLGOL

ECHO ------ BEFUNGE ------

CD C:\PROJECTS
UNZIP D:\BEFUNG93.ZIP

ECHO ------ MAENTWROG ------

CD C:\PROJECTS
UNZIP D:\MAENTWRG.ZIP

CD C:\
IF EXIST C:\AUTOEXEC.OLD GOTO AlreadyAutoexec
COPY AUTOEXEC.BAT AUTOEXEC.OLD
COPY AUTOEXEC.OLD+D:\DJGPPENV.BAT AUTOEXEC.BAT
COPY FDCONFIG.SYS FDCONFIG.OLD
COPY FDCONFIG.OLD+D:\NANSI.CFG FDCONFIG.SYS

:AlreadyAutoexec
MKDIR SKEL
XCOPY D:\SKEL C:\SKEL /S /Y /R
COPY D:\GPL20.TXT C:\SKEL\FLOPPY
ATTRIB -R C:\SKEL\FLOPPY\*.*
C:\TOOLS\UNIX2DOS C:\SKEL\FLOPPY\*.*
ATTRIB -R C:\SKEL\SHELTA\*.*
C:\TOOLS\UNIX2DOS C:\SKEL\SHELTA\*.*
ATTRIB -R C:\SKEL\ILLGOL\*.*
C:\TOOLS\UNIX2DOS C:\SKEL\ILLGOL\*.*
